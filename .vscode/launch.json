{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "typescript-debug",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["-r", "ts-node/register"],
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/${relativeFileDirname}/${fileBasename}"
    }
  ]
}
